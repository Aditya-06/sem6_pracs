   "source": [
    "print(\"Primitive Root\")\n",
    "q=int(input(\"Enter a prime no.:\"))\n",
    "\n",
    "pr=set()\n",
    "for alpha in range(1,q):\n",
    "    \n",
    "    l=[]\n",
    "    for i in range(1,q):\n",
    "        x=(alpha**i)%q\n",
    "        l.append(x)\n",
    "    \n",
    "    s=set(l)\n",
    "    if(len(l)==len(s)):\n",
    "        pr.add(alpha)\n",
    "        \n",
    "print(pr)"
   ]
